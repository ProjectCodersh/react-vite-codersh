{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        },  
        {   
            "source": "/robots.txt",
            "destination": "/robots.txt" 
        },
        { 
            "source": "/sitemap.xml",
            "destination": "/sitemap.xml" 
        }
    ],
      "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}